
{{#section 'css'}}
    <link href="/stylesheets/import/import.css" rel="stylesheet"/>
    <link href="/stylesheets/import/importPage.css" rel="stylesheet"/>
{{/section}}

<main class="statistics-container">
    <div>
    <div class="row">
        <div class="row filter">  
            <div class="col-6">
            </div>    
            <div class="col-3">
            </div>          
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-md-12 col-xs-12">
            <div class="content">             
                <table id="importTable" class="import-table">
                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Thời gian</th>
                            <th>Tổng chi phí</th>
                            <th>Số sách</th>
                            <th>Số lượng sách</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>ID</th>
                            <th>Thời gian</th>
                            <th>Tổng chi phí</th>
                            <th>Số sách</th>
                            <th>Số lượng sách</th>
                        </tr>
                    </tfoot>
                    <tbody id="importTableBody">
                        {{#each importReceipts}}
                            <tr>
                                <td>{{id}}</td>
                                <td>{{create_at}}</td>
                                <td>{{price}}</td>
                                <td>{{[import_receipt_details.count_details]}}</td>
                                <td>{{[import_receipt_details.sum_quantity]}}</td>
                            </tr>
                        {{/each}}

                        <script type="text/x-handlebars-template" id="importTableRowTemplate">

                        </script>
                    </tbody>
                </table>


            </div>
        </div>
    </div>

    </div>



    <!-- Modal -->


</main>

{{#section 'script'}}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js"></script>
    <script src="/js/helper/helper.js"></script>
    <script src="/js/import/import.js"></script>
    <script src="/js/import/importPage.js"></script>

{{/section}}